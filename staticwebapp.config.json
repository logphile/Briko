{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/health"]
  },
  "responseOverrides": {
    "404": { "rewrite": "/index.html" }
  },
  "globalHeaders": {
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff",
    "Referrer-Policy": "no-referrer-when-downgrade"
  }
}
